	clear
a	alias
caly	cal `date +%Y`
d	ls -CF --group-directories-first --color=auto
D	ls -CF --group-directories-first --color=auto -1
dte	d -trl | tail
e	vim
gd	git diff .
gs	git status
jk	cd $PROJ
k	kill -9
l	clear
m	more
q	'echo "cd "`pwd`'
qp	"pwd > ~/.qdir ; echo 'qpush'"
qj	'cd `cat ~/.qdir`'
ve	vile
pd	pushd
po	popd
res	eval `resize`
rycp	rsync --partial --progress --rsh=ssh
skey	(cd $PREFIX/tmp ; rm -fr ssh*) ; eval `ssh-agent -c` ; ssh-add
ul	'mv \!* .xtmp && cp .xtmp \!* && rm -f .xtmp'
xc	xcalc -rpn
xf	setenv SSH_AUTH_SOCK `find $PREFIX/tmp -name "agent*" -print | head -1`
xk	echo "setenv SSH_AUTH_SOCK $SSH_AUTH_SOCK"
