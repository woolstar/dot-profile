	clear
a	alias
caly	cal `date +%Y`
d	ls -CF --group-directories-first --color=auto
D	ls -CF --group-directories-first --color=auto -1
dte	d -trl | tail
e	vim
gb	git branch
gd	git diff -b .
gs	git status
gup	'git pull --rebase origin master'
jk	cd $PROJ
k	kill -9
l	clear
m	more
q	'echo "cd "`pwd`'
ve	vile
pd	pushd
po	popd
ifi	"find . -iname '*'\!*'*' -print"
mi	"make -f make.$CPU"
qj	"dirs -L ~/.qdir"
qp	"dirs -S ~/.qdir ; echo pushed"
qv	"cat ~/_places"
qe	"vim ~/_places"
ul	'mv \!* .xtmp && cp .xtmp \!* && rm -f .xtmp'
xc	'xcalc -rpn -fg white -bg midnightblue'
xf	setenv SSH_AUTH_SOCK `find $PREFIX/tmp -name "agent*" -print | head -1`
xk	echo "setenv SSH_AUTH_SOCK $SSH_AUTH_SOCK"
res	eval `resize`
rycp	rsync --partial --progress --rsh=ssh
skey	(cd $PREFIX/tmp ; rm -fr ssh*) ; eval `ssh-agent -c` ; ssh-add
