^L	clear
a	alias
caly	cal `date +%Y`
d	ls -CF --color=auto
dte	d -trl | tail
e	vim
gd	git diff .
gs	git status
jk	cd $PROJ
k	kill -9
l	clear
m	more
pd	pushd
po	popd
res	eval `resize`
rycp	rsync --partial --progress --rsh=ssh
skey	(cd /tmp ; rm -fr ssh*) ; eval `ssh-agent -c` ; ssh-add
ul	mv !* x; cp x !*; rm -f x
xc	xcalc -rpn
xf	setenv SSH_AUTH_SOCK `find /tmp -name "agent*" -print | head -1`
xk	echo "setenv SSH_AUTH_SOCK $SSH_AUTH_SOCK"
